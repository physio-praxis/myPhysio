{
	"name": "myphysio",
	"private": true,
	"version": "0.2.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"test:ci": "vitest run --coverage --reporter=dot --reporter=junit",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx --env-file=.env scripts/seed.ts",
		"db:seed:wipe": "tsx --env-file=.env scripts/seed.ts --wipe"
	},
	"devDependencies": {
		"@electric-sql/pglite": "^0.3.14",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@faker-js/faker": "^10.1.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@skeletonlabs/skeleton": "^3.2.2",
		"@skeletonlabs/skeleton-svelte": "^1.5.3",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22.19.0",
		"@types/pg": "^8.15.6",
		"@vitest/browser": "^3.2.4",
		"commander": "^14.0.2",
		"dotenv": "^17.2.3",
		"drizzle-kit": "^0.30.6",
		"drizzle-orm": "^0.40.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"playwright": "^1.56.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.43.5",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.17",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.2",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@lucide/svelte": "^0.544.0",
		"@supabase/supabase-js": "^2.80.0",
		"@vitest/coverage-v8": "^3.2.4",
		"pg": "^8.16.3",
		"zod": "^4.1.12"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"ignoredBuiltDependencies": [
			"@tailwindcss/oxide"
		]
	}
}
